<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:MathCore.NET.Samples.TCP.Server.ViewModels">

    <DataTemplate x:Key="TCPClientListBoxItem" DataType="{x:Type vm:ClientViewModel}">
        <TextBlock>
            <TextBlock Text="{Binding Client.Host}"/>:<TextBlock Text="{Binding Client.Port}"/>
        </TextBlock>
    </DataTemplate>
    
</ResourceDictionary>